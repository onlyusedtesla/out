              <input name="search" type="text" maxlength="50" placeholder="Search" class="searchbox-input js-searchbox-input" value="<%= searchQuery %>">
            <% } else { %>
              <input name="search" type="text" maxlength="50" placeholder="Search" class="searchbox-input js-searchbox-input">
