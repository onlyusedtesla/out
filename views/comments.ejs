      <div class="navigation">
        <a href="/submit" class="blue-link tooltip js-tooltip-hover" data-text="Add a story you think would be useful to the Tesla community">Add Post</a>
        <div class="searchbox js-searchbox">
          <!-- The Search Bar -->
