        <p class="username">
          <% if (userInfo) { %>
            <%= userInfo %> | <a href="/logout" class="blue-link">Sign Out</a>
          <% } else { %>
            <a href="/login" class="blue-link">
              Sign In
            </a>
          <% } %>
